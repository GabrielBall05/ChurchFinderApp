<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserIntents.EditUserProfile">

    <Button
        android:id="@+id/btn_editUserProfile_signOut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sign Out"
        android:textAlignment="center"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="574dp" />

    <Button
        android:id="@+id/btn_editUserProfile_bookmarks"
        android:layout_width="130dp"
        android:layout_height="100dp"
        android:text="Book-\nmarks"
        android:textAlignment="center"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="630dp" />

    <Button
        android:id="@+id/btn_editUserProfile_home"
        android:layout_width="130dp"
        android:layout_height="100dp"
        android:text="Home Page"
        android:textAlignment="center"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="630dp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="130dp"
        android:layout_height="100dp"
        android:text="Edit Profile"
        android:textAlignment="center"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="277dp"
        tools:layout_editor_absoluteY="630dp" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Edit Profile"
        android:textAlignment="center"
        android:textSize="32sp"
        tools:layout_editor_absoluteX="128dp"
        tools:layout_editor_absoluteY="28dp" />

    <EditText
        android:id="@+id/et_editUserProfile_fname"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="First Name"
        android:inputType="text"
        android:textAlignment="textStart"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="105dp" />

    <EditText
        android:id="@+id/et_editUserProfile_city"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="City"
        android:inputType="text"
        android:textAlignment="textStart"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="240dp" />

    <TextView
        android:id="@+id/textView38"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Password:"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="286dp" />

    <EditText
        android:id="@+id/et_editUserProfile_password"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Password"
        android:inputType="text"
        android:textAlignment="textStart"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="308dp" />

    <Button
        android:id="@+id/btn_editUserProfile_delete"
        android:layout_width="225dp"
        android:layout_height="wrap_content"
        android:text="Delete Account"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="93dp"
        tools:layout_editor_absoluteY="518dp" />

    <TextView
        android:id="@+id/tv_editUserProfile_fieldsError"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="*Please keep all fields filled out*"
        android:textAlignment="center"
        android:textColor="#FF0000"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="104dp"
        tools:layout_editor_absoluteY="425dp" />

    <Spinner
        android:id="@+id/sp_editUserProfile_denomination"
        android:layout_width="300dp"
        android:layout_height="50dp"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="375dp" />

    <Button
        android:id="@+id/btn_editUserProfile_update"
        android:layout_width="275dp"
        android:layout_height="wrap_content"
        android:text="Update Account Info"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="453dp" />

    <TextView
        android:id="@+id/textView39"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Denomination:"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="353dp" />

    <EditText
        android:id="@+id/et_editUserProfile_lname"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Last Name"
        android:inputType="text"
        android:textAlignment="textStart"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="171dp" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="City:"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="218dp" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="First Name:"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="83dp" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Last Name:"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="150dp" />

</androidx.constraintlayout.widget.ConstraintLayout>